{
  "name": "simple-panzoom",
  "version": "1.0.1",
  "type": "module",
  "description": "Extensible, mobile friendly pan and zoom framework (supports DOM and SVG).",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "test": "vitest",

    "build": "browserify -s panzoom ./index.js > dist/panzoom.js && uglifyjs dist/panzoom.js > dist/panzoom.min.js",
    "start": "npm run build"
  },
  "keywords": [
    "dom",
    "svg",
    "pan",
    "zoom"
  ],
  "author": "Andrei Kashcha",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kakajun/simple-panzoom"
  },
  "dependencies": {
    "ngraph.events": "^1.2.2",
    "wheel": "^1.0.0"
  },
  "devDependencies": {
    "vue": "^3.4.29",
    "browserify": "^17.0.0",
    "tap": "^19.2.5",
    "uglify-js": "^3.18.0",

    "@types/node": "^20.14.7",
    "@vitejs/plugin-vue": "^5.0.5",
    "@rushstack/eslint-patch": "^1.10.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.26.0",
    "vite-plugin-eslint": "^1.8.1",
    "prettier": "3.3.2",
    "sass": "^1.77.6",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "@vue/test-utils":"2.4.6",
    "jsdom": "24.1.0",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21",
    "jest": "29.7.0"
  }
}
